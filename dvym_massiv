n=int(input("Колл-во строк: "))
m=int(input("Колл-во столбцов: "))
a=[]
a=[[0]*m for i in range (n)]
print()
from random import randint
for i in range (n):
    for j in range (m):
        a[i] [j]=randint (-20,20)
        print("{:4d}".format (a[i][j]), end=' ')
    print()
